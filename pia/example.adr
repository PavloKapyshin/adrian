struct Some(a) {
    field: a
}
struct None {}
adt Maybe(a) {
    Some(a), None
}

struct LOL {
    data: py#Int

    fun ret(): Maybe(py#Int) {
        if self.data == py#Int(0) {
            return Maybe.None()
        }
        return Maybe.Some(self.data)
    }
}

let a = LOL(py#Int(0))
let b = a.ret()
if b is Some {
    py#print(b.field)
}
