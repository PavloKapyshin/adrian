sct Person {
    id: c#UIntFast64
    age: c#UIntFast8
}

sct HighPerson {
    person: ref Person

    fun __init__(person: ref Person): None {
        self.person = person
    }
}


fun doSomethingWithJohn(john: ref Person): HighPerson {
    ret HighPerson(john)
}


var john: Person = Person(c#UIntFast64(1), c#UIntFast8(20))
var secondJohn: ref Person = ref john
var anotherJohn: HighPerson = doSomethingWithJohn(secondJohn)