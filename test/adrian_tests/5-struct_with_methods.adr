sct Person {
    id: c#UIntFast32
    age: c#UIntFast8

    fun setAge(age: c#UIntFast8): None {
        self.age = age
    }

    fun addOneYear(): None {
        self.setAge(self.age + c#UIntFast8(1))
    }

    fun getAge(): c#UIntFast8 {
        ret self.age
    }
}


var john: Person = Person(c#UIntFast32(1), c#UIntFast8(30))
var jack: Person = Person(c#UIntFast32(2), c#UIntFast8(32))
var mike: Person = john  -- mike and jack have the same id!
mike.id = c#UIntFast32(3)
var mikeId: c#UIntFast32 = mike.id
var mikeAge: c#UIntFast8 = mike.getAge()
mike.addOneYear()