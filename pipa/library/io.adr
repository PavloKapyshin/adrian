struct File {
    name: String

    fun read(): String {
        return String(py#readFile(self.name.data))
    }

    fun write(contents: String): Void {
        py#writeFile(self.name.data, contents.data)
    }
}

let argv = Vector(py#List([]))
for element in py#argv {
    argv.append(String(element))
}
