struct A {
    data: py#Int

    fun dummyMethod(): Void {
        py#print(py#Str("Hello, methods!"))
    }
}


struct Container {
    value: A

    fun printMe(): Void {
        py#print(self.value.data)
    }

    fun setToZero(): Void {
        self.value.data = py#Int(0)
    }
}

let a = A(py#Int(1))
let container = Container(a)
container.value.dummyMethod()
container.printMe()
container.setToZero()
container.printMe()
