
let lexer = lexers#CombinatorLexer({
    lexers#OneOrMore(lexers#NumberCombinator()) = "number",
    lexers#OneOrMore(lexers#Or([lexers#Char("a"), lexers#Char("b")])) = "name",
    lexers#And([
        lexers#Char("'"),
        lexers#AnyChar(["'"]),
        lexers#Char("'")
    ]) = "string"
}, ignoreList = [" ",])


for (text, type) in lexer.lex("aaa bba '@@' 1234 a'123#!aba'") {
    print(" ".join([text, type]))
}
