struct Person {
    firstName: String
    secondName: String
    age: Number
    printMe: Function(Void)
}

fun printMike(): Void {
    print(" ".join([self.firstName, self.secondName]))
}

fun anotherPrintingFunc(): Void {
    print(" ".join([self.firstName, self.secondName, "age =", self.age]))
}

let mike = Person("Mike", "Joker", 25, printMike)
mike.printMe()
mike.printMe = anotherPrintingFunc
mike.printMe()
