fun factorialRecurse(n: Number): Number {
    if n in [0, 1] {
        return 1
    }
    let innerResult = factorialRecurse(n - 1)
    return innerResult * n
}

fun factorialLooped(n: Number): Number {
    if n in [0, 1] {
        return 1
    }
    return factorialLooped(n - 1) * n
}


